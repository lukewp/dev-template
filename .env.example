# Example .env file for template projects

# General project settings
PROJECT_NAME="dev-template"
ENVIRONMENT="development"

# API keys and tokens (do not commit real values)
API_KEY="your-api-key-here"
OPENAI_API_KEY="your-openai-api-key-here"

# Database settings
DB_HOST="localhost"
DB_PORT="5432"
DB_USER="your_db_user"
DB_PASSWORD="your_db_password"
DB_NAME="dev_db"

# Logging
LOG_LEVEL="INFO"

# Feature flags
ENABLE_EXPERIMENTAL_FEATURES="false"

# External services
S3_BUCKET="your-bucket-name"
GCS_PROJECT_ID="your-google-project-id"

# Secrets below should be populated in your actual .env file only
